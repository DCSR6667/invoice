<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css">
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.4/dist/jquery.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="/view_data.css">
</head>
<body>

    <div class="row box">
        <div class="col-3 border box1 shadow-sm ">
            <% for(var i=0;i<data.length/2;i++) { var j=JSON.stringify(data[i]); %>

                <div class="row item">
                    <div class="col-8"><span><%= data[i].Name %></span></div>
                    <div class="col-2"> <a class="btn btn-primary" href="/generate/<%=j %>">payslip</a></div>
                </div>

               

                <% }%>
        </div>
        <div class="col-3 border box2 shadow-sm">

            <% for(var i=Math.ceil(data.length/2);i<data.length;i++) { var j=JSON.stringify(data[i]); %>

                <div class="row item">
                    <div class="col-8"><span><%= data[i].Name %></span></div>
                    <div class="col-2"> <a class="btn btn-primary" href="/generate/<%=j %>">payslip</a></div>
                </div>


                <% }%>


        </div>
    </div>



    
    
</body>
</html>